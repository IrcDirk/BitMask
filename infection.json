{
    "timeout": 1,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "build/infection/text.log",
        "summary": "build/infection/summary.log",
        "debug": "build/infection/debug.log",
        "perMutator": "build/infection/perMutator.md",
        "badge": {
            "branch": "master"
        }
    },
    "mutators": {
        "@default": true,
        "DecrementInteger": {
            "ignore": [
                "BitMask\\AssociativeBitMask::__construct::37"
            ]
        },
        "OneZeroInteger": {
            "ignore": [
                "BitMask\\AssociativeBitMask::__construct::37",
                "BitMask\\AssociativeBitMask::set::56"
            ]
        },
        "LessThan": {
            "ignore": [
                "BitMask\\IndexedBitMask::set::37"
            ]
        },
        "FalseValue": {
            "ignore": [
                "BitMask\\AssociativeBitMask::__construct::37"
            ]
        },
        "MethodCallRemoval": {
            "ignore": [
                "BitMask\\AssociativeBitMask::__construct::36"
            ]
        }
    }
}
