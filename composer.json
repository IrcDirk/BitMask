{
  "name": "yaroslavche/bitmask",
  "description": "BitMask, IndexedBitMask, AssociativeBitMask",
  "license": "MIT",
  "keywords": [
    "bit",
    "bitmask",
    "binary",
    "bitwise",
    "php",
    "php7"
  ],
  "require-dev": {
    "behat/behat": "^3.4",
    "phpunit/phpunit": "^7.0",
    "symfony/var-dumper": "^4.0",
    "leanphp/behat-code-coverage": "^3.4",
    "phpbench/phpbench": "master",
    "phpstan/phpstan": "^0.11.8",
    "squizlabs/php_codesniffer": "^3.4",
    "thecodingmachine/safe": "^0.1.13",
    "thecodingmachine/phpstan-safe-rule": "^0.1.2",
    "thecodingmachine/phpstan-strict-rules": "^0.11.0",
    "doctrine/dbal": "^2.9",
    "infection/infection": "^0.13.1"
  },
  "authors": [
    {
      "name": "yaroslavche",
      "email": "yaroslav429@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "BitMask\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "BitMask\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "cscheck": "phpcs --ignore=features/**",
    "csfix": "phpcbf --ignore=features/**",
    "phpstan": "phpstan analyse src/ -c phpstan.neon --level=7 --no-progress -vvv --memory-limit=1024M",
    "benchmarks": "phpbench run benchmarks --report=default",
    "infection": "infection --min-msi=50 --min-covered-msi=70",
    "behat": "behat"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
